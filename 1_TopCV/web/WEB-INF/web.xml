<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>AdminManageAccountServlet</servlet-name>
        <servlet-class>controller.AdminManageAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JobSeekerProfileServlet</servlet-name>
        <servlet-class>controller.jobseeker.JobSeekerProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JobSeekerJobListServlet</servlet-name>
        <servlet-class>controller.JobSeekerJobListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminManageAccountServlet</servlet-name>
        <url-pattern>/adminmanageaccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JobSeekerProfileServlet</servlet-name>
        <url-pattern>/jobseekerprofile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JobSeekerJobListServlet</servlet-name>
        <url-pattern>/jobseekerjoblist</url-pattern>
    </servlet-mapping>
</web-app>
